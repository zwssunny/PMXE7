  {
  private:
	TWriteUserLog FWriteUserLog;
	String FUserID;
	String FIPAddress;
	String FAcckBook ;
	int Ftype;
	String FDesc;
	String FClassCode;
  public:
	TLogQueueProc(TWriteUserLog& AWriteUserLog,String AUserID, String AIPAddress, String AClassCode,String AAcckBook, int Atype, String ADesc);
	void __fastcall Execute();
};
